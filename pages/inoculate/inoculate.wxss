
.baby{
    height:200rpx;
    display:flex;
    flex-direction:column;
    justify-content:center;
    align-items:center;
}
.baby>view{
     display:flex;
     align-items: center;
     margin-bottom:20rpx;
}
.baby image:nth-of-type(1) {
    width:100rpx;
    height:100rpx;
    border-radius:50%;
    margin-right:20rpx;

}
.baby image:nth-of-type(2){
   width:40rpx;
   height:40rpx;
}
.arrow {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.main{
  background:#eee;
  font-size:30rpx;
  height:100vh;
  padding-top:2rpx;
}
.main>view{
  background:#fff;
  padding-left:20rpx;
}
.cmmon{
  padding:20rpx 0;
  display:flex;
  justify-content:space-between;
  align-items: center;
  border-bottom:2rpx solid #eee;
}
.cmmon:nth-of-type(2){
  border:none;
}
