
.container{
  height: 100vh;
}
.userinfo-avatar{
   width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  display: inline-block;
}
.icon_arrow{
   width: 40rpx;
   height:40rpx;
   display:inline-block;
   background-image: url("data:image/svg+xml;base64,");
   background-size: cover;
}
.my_avatar{
  height:200rpx;
  display:flex;
  align-items: center;
  justify-content: space-between;
}
.icon_baby {
  background-image: url('data:image/svg+xml;base64,');
  background-size: cover;
}
.icon_order{
   background-size: cover;
    background-image: url('data:image/svg+xml;base64,')
}
.icon_recommend{
    background-size: cover;
    background-image: url('data:image/svg+xml;base64,')
}
.icon_set{
  background-size: cover;
  background-image: url('data:image/svg+xml;base64,')
}
.icon_common{
  width: 60rpx;
  height: 60rpx;
  margin: 20rpx;
  border-radius: 0;
}

.my_avatar>view {
    display:flex;
    align-items: center;
}
.my_avatar>text{
    margin-right:20rpx;
}
.under_line{
  display:block;
  width:100%;
  height:6rpx;
  background: #eee;
}
.my_baby .userinfo-avatar, .recommend .userinfo-avatar {
    width: 60rpx;
  height: 60rpx;
  margin: 20rpx;
  border-radius: 0;
}
.my_baby{
  height:130rpx !important;
}
 .recommend{
    height:160rpx !important;
 }